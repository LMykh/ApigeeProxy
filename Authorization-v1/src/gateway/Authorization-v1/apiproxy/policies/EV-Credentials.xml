<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="EV-Credentials">
    <DisplayName>EV-Credentials</DisplayName>
    <Header name="Authorization">
        <Pattern ignoreCase="false">{basic}</Pattern>
    </Header>
    <JSONPayload>
        <Variable name="login">
            <JSONPath>$.login</JSONPath>
        </Variable>
        <Variable name="password">
            <JSONPath>$.password</JSONPath>
        </Variable>
    </JSONPayload>
    <VariablePrefix>userCredent</VariablePrefix>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>